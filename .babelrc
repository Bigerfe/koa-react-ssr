{
    "env": {
        "production": {
            "presets": ["@babel/preset-env","@babel/preset-react"]
        },
        "development":{
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react"
            ],
            "plugins":["@babel/plugin-syntax-dynamic-import","@babel/plugin-proposal-class-properties",
                [
                    "import",
                    {
                        "libraryName": "antd",
                        "libraryDirectory": "es",
                        "style": "css"
                    }
                ]
            ]
        }
    }
}